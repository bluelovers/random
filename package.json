  "homepage": "https://github.com/bluelovers/random#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/random/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/random.git"
  },
  "license": "MIT",
  "author": "Travis Fischer <travis@automagical.ai>",
  "main": "index.js",
  "module": "src/random.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "benchmark": "npx ts-node ./test/benchmark/base.perf.ts && npx ts-node ./test/benchmark/random-id.perf.ts",
    "docs": "update-markdown-jsdoc -f src/random.js --shallow",
    "prepublishOnly": "npm run docs && npm run test && npx sort-package-json",
    "test": "ava --serial -v test/**/*.test.js"
  },
  "dependencies": {
    "@types/seedrandom": "^2",
    "hash-sum": "^1.0.2",
    "nanoid": "^1.3.1",
    "ow-lite": "0.0.2",
    "seedrandom": "^2.4.4",
    "shortid": "^2.2.13",
    "uni-string": "^1.1.0"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4",
    "ava": "^0.25.0",
    "benchmark": "^2.1.4",
    "chai": "^4",
    "debug-color2": "^1.0.6",
    "math-random": "^1.0.1",
    "microtime": "^2.1.8",
    "ts-enum-util": "^3.1.0",
    "update-markdown-jsdoc": "^1.0.6"
  },
  "engines": {
    "node": ">=7"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "reveal": true
}
